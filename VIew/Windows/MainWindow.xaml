<Window
    x:Class="KFCMenu.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:fawesome="http://schemas.fontawesome.com/icons/"
    xmlns:local="clr-namespace:KFCMenu"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:mvm="clr-namespace:KFCMenu.ViewModel"
    xmlns:pages="[pages ]clr-namespace:KFCMenu.VIew"
    Title="{Binding Title}"
    Width="800"
    Height="600"
    IsManipulationEnabled="False"
    ResizeMode="NoResize"
    mc:Ignorable="d">



    <Window.DataContext>
        <mvm:MainViewModel />
    </Window.DataContext>

    <Grid />

</Window>
